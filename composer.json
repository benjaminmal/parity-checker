{
    "name": "benjaminmal/parity-checker",
    "description": "Parity checker",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Malgras",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/options-resolver": "^5.4 || ^6.0",
        "symfony/property-access": "^5.4 || ^6.0",
        "symfony/property-info": "^5.4 || ^6.0",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9.7",
        "e-lodgy/coding-standard": "^1.1.5",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan-symfony": "^1.2.19",
        "phpstan/phpstan-webmozart-assert": "^1.2.2",
        "symfony/var-dumper": "^5.4.14 || ^6.1.6",
        "phpunit/phpunit": "^9.5.27",
        "phpstan/phpstan-phpunit": "^1.3.3",
        "php-parallel-lint/php-var-dump-check": "^0.5.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "Benjaminmal\\ParityChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Benjaminmal\\ParityChecker\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "mv .gitattributes.disable .gitattributes"
        ]
    }
}
